       <mtd columnalign="right" rowalign="center">
        <mrow>
         <mi>g</mi><mo stretchy="false">(</mo><mi>f</mi><mo stretchy="false">(</mo><mi>x</mi><mo stretchy="false">)</mo><mo stretchy="false">)</mo>
        </mrow>
       </mtd>
