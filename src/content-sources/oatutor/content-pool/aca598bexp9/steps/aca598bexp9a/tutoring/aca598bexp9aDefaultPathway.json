        "type": "hint",
        "dependencies": [
            "aca598bexp9a-h5"
        ],
        "title": "Simplification",
        "text": "Divide by $$49$$ on both sides, we get $$\\frac{{\\left(x-1\\right)}^3}{x^3-3x-10}=1$$",
        "variabilization": {},
        "oer": "https://OATutor.io",
        "license": ""
