    "dependency-check": "^3.3.0",
    "hallmark": "^3.1.0",
    "memdown": "^6.1.0",
    "nyc": "^15.1.0",
    "reachdown": "^1.0.0",
    "standard": "^16.0.3",
    "tape": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Level/deferred-leveldown.git"
  },
  "homepage": "https://github.com/Level/deferred-leveldown",
