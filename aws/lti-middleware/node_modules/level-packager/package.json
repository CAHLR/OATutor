  "files": [
    "abstract",
    "level-packager.js",
    "CHANGELOG.md",
    "UPGRADING.md"
  ],
  "dependencies": {
    "encoding-down": "^7.1.0",
    "levelup": "^5.1.1"
  },
  "devDependencies": {
    "airtap": "^4.0.3",
    "airtap-playwright": "^1.0.1",
    "dependency-check": "^3.3.0",
    "hallmark": "^3.1.0",
    "nyc": "^15.1.0",
    "standard": "^16.0.3",
    "tape": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Level/packager.git"
  },
  "homepage": "https://github.com/Level/packager",
  "keywords": [
    "level",
    "leveldb",
    "stream",
    "database",
    "db",
    "store",
    "storage",
    "json"
