  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "generate": "(cd src/util/; node indexGenerator.js; cd ../..; npm run start)  || (echo \"[OpenITS INFO] Ignore the path cannot be found warning, it's expected for Windows.\" && cd src/util/ && node indexGenerator.js && cd ../.. && npm run start)",
